{
  "name": "event-manager-application",
  "version": "1.0.0",
  "description": "An application that allow users to book events and manage the event that they create.",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec ./node_modules/.bin/babel-node ./server/app",
    "build": "webpack",
    "start": "webpack-dev-server",
    "test": "npm run reset:db && npm run migrate:test && npm run seed:test && nyc mocha --require babel-polyfill --compilers js:babel-register ./server/test/test.js",
    "coverage": "nyc --all npm test && nyc report --reporter=html --reporter=text-summary",
    "seed:test": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:seed:all",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "migrate:test": "./node_modules/.bin/cross-env NODE_ENV=test sequelize db:migrate",
    "reset:db": "./node_modules/.bin/cross-env NODE_ENV=test node ./server/test/resetdb.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blaize3/event-manager-application.git"
  },
  "keywords": [],
  "author": "Ode Akugbe",
  "license": "MIT ",
  "bugs": {
    "url": "https://github.com/Blaize3/event-manager-application/issues"
  },
  "homepage": "https://github.com/Blaize3/event-manager-application#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^3.1.0",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "mocha": "^4.0.1",
    "node-sass": "^4.6.1",
    "nyc": "^11.3.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
